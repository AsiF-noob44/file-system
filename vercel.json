{
  "version": 2,
  "builds": [
    {
      "src": "BackEnd/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "FrontEnd/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BackEnd/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "FrontEnd/$1"
    }
  ],
  "functions": {
    "BackEnd/server.js": {
      "maxDuration": 10
    }
  }
}
